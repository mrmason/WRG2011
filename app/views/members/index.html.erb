<h1>Listing members</h1>

<table>
  <tr>
    <th>Team</th>
    <th>Player</th>
    <th>Price paid</th>
    <th>Start week</th>
    <th>End week</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @members.each do |member| %>
  <tr>
    <td><%= member.team_id %></td>
    <td><%= member.player_id %></td>
    <td><%= member.price_paid %></td>
    <td><%= member.start_week %></td>
    <td><%= member.end_week %></td>
    <td><%= link_to 'Show', member %></td>
   </tr>
<% end %>
</table>

<br />

