<h2>Players</h2>

<table class="view">
  <tr>
    <th>FFID</th>
    <th>Name</th>
    <th>Status</th>
    <th>Team</th>
    <th>Value</th>
    <th>Total Points</th>
    <th>Gameweek Points</th>
  </tr>

<% @players.each do |player| %>
  <tr>
    <td><%= link_to player.ff_id, player%></td>
    <td><%= link_to player.full_name, player %></td>
    <td><%= player.status %>
    <td><%= player.team %></td>
    <td><%= player.value %></td>
    <td><%= player.total_points %></td>
    <td><%= player.gameweek_points %></td>
  </tr>
<% end %>
</table>

<br />

